<resources>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="action_bar_icon">
        <item name="android:contentDescription">动作导航条icon的Style</item>
        <item name="android:layout_width">@dimen/actionbar_icon_width</item>
        <item name="android:layout_height">@dimen/actionbar_icon_heigth</item>
        <item name="android:background">@drawable/home_entry_item</item>
        <item name="android:onClick">onClick</item>
        <item name="android:clickable">true</item>
    </style>

    <!-- 通讯大全View -->
    <style name="tel_showtext">
        <item name="android:background">#FF009ACD</item>
        <item name="android:gravity">center</item>
        <item name="android:text">一站式号码服务平台</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/tel_show_textsize</item>
        <item name="android:textStyle">bold</item>
    </style>

    
</resources>